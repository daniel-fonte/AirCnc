{
  "name": "Api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.8.7",
    "sucrase": "^3.12.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "husky": "^4.0.9",
    "lint-staged": "^9.5.0",
